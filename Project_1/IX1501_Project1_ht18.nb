       RowBox[{"1", "/", "6"}], ",", 
       RowBox[{"1", "\[LessEqual]", "k", "\[LessEqual]", "6"}]}], "}"}], 
     "}"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
